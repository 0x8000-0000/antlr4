            str = ctx.INT(0).symbol.text + ' ' + ctx.INT(1).symbol.text + ' ' + antlr4.Utils.arrayToString(ctx.INT());
        } else {
            str = ctx.ID().symbol.toString();
        }
    	document.getElementById('output').value += str + '\\n';
    };
    return this;
};
