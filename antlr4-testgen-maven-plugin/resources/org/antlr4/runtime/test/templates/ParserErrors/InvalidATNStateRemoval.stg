TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "x:x"

Rule() ::= "start"

Output() ::= ""

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
start : ID ':' expr;
expr : primary expr? {<Pass()>} | expr '->' ID;
primary : ID;
ID : [a-z]+;
>>
