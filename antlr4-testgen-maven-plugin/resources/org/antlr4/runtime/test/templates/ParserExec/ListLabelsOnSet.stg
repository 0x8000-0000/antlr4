TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "abc 34;"

Rule() ::= "a"

Output() ::= ""

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
a : b b* ';' ;
b : ID val+=(INT | FLOAT)*;
ID : 'a'..'z'+ ;
INT : '0'..'9'+;
FLOAT : [0-9]+ '.' [0-9]+;
WS : (' '|'\n') -> skip ;
>>
