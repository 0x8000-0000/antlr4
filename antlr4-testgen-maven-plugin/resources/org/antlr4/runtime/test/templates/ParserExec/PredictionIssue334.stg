TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "a"

Rule() ::= "file_"

Output() ::= <<
(file_ (item a) \<EOF>)<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
file_ @init{
<BailErrorStrategy()>
} 
@after {
<ToStringTree("$ctx"):writeln()>
}
  :   item (SEMICOLON item)* SEMICOLON? EOF ;
item : A B?;
SEMICOLON: ';';
A : 'a'|'A';
B : 'b'|'B';
WS      : [ \r\t\n]+ -> skip;
>>
