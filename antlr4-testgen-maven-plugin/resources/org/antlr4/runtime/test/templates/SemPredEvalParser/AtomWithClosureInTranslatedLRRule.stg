TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "a+b+a"

Rule() ::= "start"

Output() ::= ""

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
start : e[0] EOF;
e[int _p]
    :   ( 'a' | 'b'+ ) ( {3 >= $_p}? '+' e[4] )*
    ;

>>
